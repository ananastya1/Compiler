type numbers is array[5] integer;
numbers[1] := 10
numbers[2] := 20
numbers[3] := 30 + 234*12
numbers[4] := 40
numbers[5] := 50
var target: integer is 30;
var found: boolean is false;
for i in 1..5 loop
if numbers[i] = target then
found := true;
end;
end;
if found then
write(true);
else
write(false);
end;